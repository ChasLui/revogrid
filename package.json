    "prettier": "2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0",
    "svelte": "^3.58.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.47",
    "bootstrap": "^5.2.3"
  },
  "dependencies": {
    "@stencil/core": "^3.3.0",
    "lodash": "^4.17.21"
  }
